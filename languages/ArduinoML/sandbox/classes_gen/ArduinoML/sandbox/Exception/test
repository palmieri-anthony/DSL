const int THRESHOLD_LOW=500;const int THRESHOLD_HIGH = 520;
void mainly ();
void tt ();
void exception_1243t ();
void setup() {
  pinMode(OUTPUT,12);
  pinMode(OUTPUT,54);
}
void mainly() {
}
void tt() {
  if (digitalRead(12)==HIGH) {
    exception_1243t();
  }
}
void exception_1243t() {
  if (digitalRead(54)==LOW) {
    digitalWrite(54,HIGH);
    delay(500);
    digitalWrite(54,LOW);
    delay(500);
    digitalWrite(54,HIGH);
    delay(500);
    digitalWrite(54,LOW);
    delay(500);
    delay(500);
    exception_1243t();
  }
}
void loop() {
mainly();
}